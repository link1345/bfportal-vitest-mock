{
  "name": "bfportal-vitest-mock",
  "version": "0.0.12",
  "description": "bfportal-vitest-mock is a mocking tool that makes it easier to unit test Battlefield 6 Portal (BF Portal) TypeScript scripts with Vitest.",
  "license": "MIT",
  "author": "PlumRice",
  "repository": {
    "type": "git",
    "url": "https://github.com/link1345/bfportal-vitest-mock"
  },
  "keywords": [
    "vitest",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/link1345/bfportal-vitest-mock/issues"
  },
  "homepage": "https://github.com/link1345/bfportal-vitest-mock",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "bfportal-vitest-mock": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json -d",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint ./src --fix",
    "check:cli": "tsx src/cli.ts generate --sdk ./code/mod/index.d.ts --out ./bfportal-vitest-mock.generated.ts",
    "check:test": "vitest -c ./test/vitest.config.ts"
  },
  "dependencies": {
    "ts-morph": "^27.0.2",
    "vitest": "^4.0.8"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}